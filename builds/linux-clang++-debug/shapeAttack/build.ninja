      -I/home/dwalker0044/Projects/ShapeAttack/lib/imgui/examples $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/imgui/examples/libs/gl3w
  flags = -std=c++20 -O1 -g
  defines = -DSPDLOG_FMT_EXTERNAL -DIMGUI_IMPL_OPENGL_LOADER_GL3W
  exe_name = ShapeAttack.exe
  linker_args = -Wl,-rpath='$$ORIGIN' $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/fmt $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/spdlog $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/imgui $
      -lGL -ldl -lSDL2 -lfmt -lspdlog -limgui

build shapeAttack: phony ShapeAttack.exe


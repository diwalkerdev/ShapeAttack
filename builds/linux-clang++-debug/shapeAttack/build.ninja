include $
    /home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/rules.ninja

subninja $
    /home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/kissSDL/build.ninja

build $
    /home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/shapeAttack/main.o: $
    compile /home/dwalker0044/Projects/ShapeAttack/src/main.cpp
  compiler = clang++
  includes = -I/usr/include -I/usr/include/SDL2 $
      -I/home/dwalker0044/Projects/ShapeAttack/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/LinAlg/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/fmt/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/spdlog/include $
      -I/usr/include -I/usr/include/SDL2 $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/kiss_sdl
  flags = -std=c++20 -O1 -g
  defines = -DSPDLOG_FMT_EXTERNAL $
      -DRESDIR=\"/home/dwalker0044/Projects/ShapeAttack/lib/kiss_sdl/\"

build ShapeAttack.exe: exe $
    /home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/shapeAttack/main.o $
    | libkiss_sdl.a
  compiler = clang++
  includes = -I/usr/include -I/usr/include/SDL2 $
      -I/home/dwalker0044/Projects/ShapeAttack/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/LinAlg/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/fmt/include $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/spdlog/include $
      -I/usr/include -I/usr/include/SDL2 $
      -I/home/dwalker0044/Projects/ShapeAttack/lib/kiss_sdl
  flags = -std=c++20 -O1 -g
  defines = -DSPDLOG_FMT_EXTERNAL $
      -DRESDIR=\"/home/dwalker0044/Projects/ShapeAttack/lib/kiss_sdl/\"
  exe_name = ShapeAttack.exe
  linker_args = -Wl,-rpath='$$ORIGIN' $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/kissSDL $
      -lkiss_sdl $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/fmt $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/spdlog $
      -L/home/dwalker0044/Projects/ShapeAttack/builds/linux-clang++-debug/imgui $
      -lSDL2_image -lSDL2_ttf -lSDL2 -lfmt -lspdlog

build shapeAttack: phony ShapeAttack.exe

